<section><h2>Token list</h2>
    <table>
        <thead>
        <tr>
            <th>Name</th>
            <th>Email</th>
            <th></th>
        </tr>
        </thead>
        <tbody>
        {{#tokenList}}
            <tr>
                <td>{{name}}</td>
                <td>{{email}}</td>
                <td>
                    <a href="/admin/token/{{_id}}" btn primary>Show</a>
                </td>
            </tr>
        {{/tokenList}}
        </tbody>
    </table>
</section>
